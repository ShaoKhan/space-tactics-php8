{% block mainmenu %}
        <div class="container-fluid">
            <div class="row">

                <div class="col-12">
                    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
                        <a class="navbar-brand" href="#">
                            <img src="{{ asset('img/logo.png') }}" alt="Space-Tactics Logo">
                        </a>
                        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                            <span class="navbar-toggler-icon"></span>
                        </button>

                        <div class="collapse navbar-collapse" id="navbarSupportedContent">
                            <ul class="navbar-nav mr-auto">
                                <li class="nav-item">
                                    <span class="nav-link" href="#">Metall</span>
                                </li>
                                <li class="nav-item">
                                    <span class="nav-link" href="#">Kristall</span>
                                </li>
                                <li class="nav-item">
                                    <span class="nav-link" href="#">Deuterium</span>
                                </li>
                                <li class="nav-item">
                                    <span class="nav-link" href="#">DM</span>
                                </li>
                                {% if app.user is not null %}
                                    <li class="nav-item">
                                        <a href="{{ path('logout') }}" class="nav-link text-danger">Logout</a>
                                    </li>
                                {% endif %}
                            </ul>
                        </div>
                    </nav>

                </div>
            </div>
        </div>
{% endblock %}